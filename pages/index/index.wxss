.index_swiper swiper {
  height: 340rpx;
}
.index_nav {
  display: flex;
}
.index_nav navigator {
  flex: 1;
  padding: 20px;
}
.index_floor .floor_item .item_content {
  overflow: hidden;
}
.index_floor .floor_item .item_content .item_content1 {
  width: 33%;
  float: left;
  border-left: 5rpx solid #fff;
  border-right: 5rpx solid #fff;
}
.index_floor .floor_item .item_content .item_content1:nth-last-child(-n+4) {
  height: 207.97413793rpx;
}
.index_floor .floor_item .item_content .item_content1:nth-last-child(-n+2) {
  border-top: 5rpx solid #fff;
}
.index_floor .floor_item .item_content .item_content1 image {
  height: 100%;
}
